<application>
    <name>teoGravityCompensator App</name>

    <module>
        <name>launchManipulation</name>
        <node>/manipulation</node>
    </module>

    <module>
        <name>launchLocomotion</name>
        <node>/locomotion</node>
    </module>

    <module>
        <name>teoGravityCompensator</name>
        <parameters>--kinematics rightLegKinematics.ini --remote /teo/rightLeg</parameters>
        <node>localhost</node>
    </module>

    <module>
        <name>teoGravityCompensator</name>
        <parameters>--kinematics leftLegKinematics.ini --remote /teo/leftLeg</parameters>
        <node>localhost</node>
    </module>

    <module>
        <name>teoGravityCompensator</name>
        <parameters>--kinematics rightArmKinematics.ini --remote /teo/rightArm</parameters>
        <node>localhost</node>
    </module>

    <module>
        <name>teoGravityCompensator</name>
        <parameters>--kinematics leftArmKinematics.ini --remote /teo/leftArm</parameters>
        <node>localhost</node>
    </module>

</application>

